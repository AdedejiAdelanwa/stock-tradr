{
    "env": {
        "browser": true,
        "jest": true,
        "es6": true
    },
    "plugins": ["import"],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "requireConfigFile": false,
        "babelOptions": {
            "presets": ["@babel/preset-react"]
        }
    },
    "parser": "@babel/eslint-parser",
    "rules": {
        "no-console": "warn",
        "no-eval": "error",
        "import/first": "error",
        "no-undef": "warn",
        "indent": "off",
        "react/prop-types": ["off"]
    }
}
